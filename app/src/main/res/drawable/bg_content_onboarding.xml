<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#fffafa"/>
            <corners android:topRightRadius="32dp" android:topLeftRadius="32dp" />
        </shape>
    </item>
    <item
        android:left="1dp"
        android:right="1dp"
        android:top="2dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
            <corners android:topRightRadius="32dp" android:topLeftRadius="32dp" />
        </shape>
    </item>

</layer-list>